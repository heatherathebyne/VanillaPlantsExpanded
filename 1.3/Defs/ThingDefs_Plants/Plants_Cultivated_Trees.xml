<Defs>

	<!--=========================== Fruit Trees ==============================-->

	<ThingDef ParentName="TreeBase">
		<defName>VCE_AppleTree</defName>
		<label>apple tree</label>
		<description>A tree bearing glossy, red apples, while a myriad of different varieties exist this particular type favors warm summers and mild winters. Apples have been used as snacking food, a dessert ingredient, and brewed into cider since early mankind.</description>
		<graphicData>
			<texPath>Things/Plant/Trees/VCE_TreeApple</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.3, 0.15)</volume>
				<offset>(0,0,-0.38)</offset>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<statBases>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
			<Nutrition>1.25</Nutrition>
		</statBases>
		<ingestible />
		<plant>
			<dieIfLeafless>false</dieIfLeafless>
			<growDays>32.51</growDays>
			<visualSizeRange>0.8~2.0</visualSizeRange>
			<sowMinSkill>6</sowMinSkill>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.71</fertilityMin>
			<topWindExposure>0.15</topWindExposure>
			<mustBeWildToSow>true</mustBeWildToSow>
			<immatureGraphicPath>Things/Plant/Trees/VCE_TreeApple_Immature</immatureGraphicPath>
			<leaflessGraphicPath>Things/Plant/Trees/VCE_TreeApple_Leafless</leaflessGraphicPath>
			<harvestWork>800</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestYield>50</harvestYield>
			<harvestedThingDef>VCE_RawApple</harvestedThingDef>
			<harvestFailable>true</harvestFailable>
			<harvestAfterGrowth>0.20</harvestAfterGrowth>
		</plant>
	</ThingDef>

